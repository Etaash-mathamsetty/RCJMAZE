#include "MeOrion.h" 
Servo myservo; 
int16_t servopin = "A1"; 

void setup()
{
  myservo.attach(servopin);  // attaches the servo on servopin
}

void loop()
{   
 for(int k = 1; k <= 3; k++) {
  for(int i = 0; i < 4; i++) { 
  myservo.write(60 * k);        
  delay(100);            
  myservo.write(0);         
  delay(100);      
  }    
 }          
}
